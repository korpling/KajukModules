<!ELEMENT doc  (pb | line | lb | subj | praed | J | IP| Inf | KOR | FOK | VOR | SUB | obj | ADV | XX)* >
<!ATTLIST doc
        ID ID #REQUIRED
>

<!ELEMENT line  ANY>
<!ATTLIST line 
	n CDATA #REQUIRED
>

<!ELEMENT pb  ANY>
<!ATTLIST pb 
	n CDATA #IMPLIED
>
<!ENTITY % element "subj | J | FOK | VOR | SUB | IP | Inf | KOR | obj | praed | ADV | XX">

<!ELEMENT lb  (#PCDATA | %element; | lb )*>
<!ATTLIST lb 
	n CDATA #IMPLIED
	type (WR | E | subj | subjattr | obj) #IMPLIED
	ADDtype CDATA #IMPLIED
	IR (adv | disj | exp | fin | inst | kaus | kom | komp | kond | kons | konz | kop | meta | rest | temp) #IMPLIED
	ADDIR CDATA #IMPLIED
	EB (prag) #IMPLIED
>
<!ELEMENT subj  (#PCDATA) >
<!ATTLIST subj
	real  CDATA "Pron"
	type (E | noE | abs) #IMPLIED
	ADDtype CDATA #IMPLIED
	change CDATA #IMPLIED
	viol CDATA #IMPLIED
	dir (V | R )  #IMPLIED
>
<!ATTLIST praed
	real (mikro | afin) #IMPLIED
	type (E | noE) #IMPLIED
	change CDATA #IMPLIED
	viol CDATA #IMPLIED
	dir (V | R )  #IMPLIED
>
<!ELEMENT praed  (V | VP | RF | NGr | PGr | Phras | ADJGr)* >
<!ELEMENT V  (#PCDATA | HV | VV | KV | MV | HMV | FV | PV | LASSEN | TUN | AcI )* > 
<!ATTLIST V 
	ID (Fin | Inf | PII | PI |InfZU| InfAM) #REQUIRED
	type (E | noE) #IMPLIED
	change CDATA #IMPLIED
	viol CDATA #IMPLIED
	dir (V | R )  #IMPLIED
>
<!ELEMENT J  (AP | KON | SUB | AD | FOK )* >
<!ATTLIST J
	IR (adv | disj | exp | fin | inst | kaus | kom | komp | kond | kons | konz | kop | meta | rest | temp) #REQUIRED
	EB (prag) #IMPLIED
	ADDIR CDATA #IMPLIED
	norm CDATA #IMPLIED
	ID CDATA #IMPLIED
	type (E | noE | x) #IMPLIED
	change CDATA #IMPLIED
	viol CDATA #IMPLIED
	dir (V | R )  #IMPLIED
>
<!ATTLIST AP
	type (KOR | FOK | x) #IMPLIED
	ADDtype CDATA #IMPLIED
>
<!ATTLIST VOR
	type (E | noE) #IMPLIED
	dir (V | R )  #IMPLIED
>
<!ELEMENT obj  (#PCDATA | akk | obl | prae)* >


<!ATTLIST HV
	type (E | noE) #IMPLIED
	change CDATA #IMPLIED
	viol CDATA #IMPLIED
	dir (V | R )  #IMPLIED
>
<!ATTLIST FV
	type (E | noE) #IMPLIED
	change CDATA #IMPLIED
	viol CDATA #IMPLIED
	dir (V | R )  #IMPLIED
>
<!ATTLIST Pras
	type (E | noE) #IMPLIED
	change CDATA #IMPLIED
