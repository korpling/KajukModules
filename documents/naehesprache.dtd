	viol CDATA #IMPLIED
	dir (V | R )  #IMPLIED
>
<!ATTLIST SUB
	type (E | noE | subj | x ) #IMPLIED
	ADDtype CDATA #IMPLIED
	ID CDATA #IMPLIED
	IR (zero) #IMPLIED
	ADDIR CDATA #IMPLIED
	norm CDATA #IMPLIED
	change CDATA #IMPLIED
	viol CDATA #IMPLIED
	dir (V | R )  #IMPLIED
>
<!ATTLIST AD
	type (x) #IMPLIED
>
<!ATTLIST FOK
	type (E | noE) #IMPLIED
