					<V ID="Fin"><MV>mochten</MV></V>
				</praed>
				<praed>
					<V ID="Inf"><VV>bekomen,</VV></V>
				</praed><newline></newline> ausgenomen Marpurg,</lb>
		</line>
		<line n="6">
			<lb n="3">
				<subj>das</subj>
				<praed>
					<V ID="Fin"><KV>war</KV></V>
				</praed>
				<praed>
					<ADJGr>sicher</ADJGr>
				</praed> das Mal,</lb>
			<lb n="3">
				<J IR="kaus" norm="denn">
					<KON>dan</KON>
				</J>
				<subj>die Völker</subj>
				<praed>
					<V ID="Fin"><VV>lagen</VV></V>
				</praed> in den Dörfern und Städen,</lb><newline></newline>
		</line>
		<line n="7">
			<lb>
				<SUB IR="zero">so</SUB> zu dem Haus Marpurg <praed>
					<V ID="Fin"><VV>gehöreten.</VV></V>
				</praed></lb>
			<lb n="4">
				<J IR="temp">
					<SUB>Wie</SUB>
				</J>
				<subj real="Pron">sie</subj>
				<FOK>nun</FOK> das Mal <praed>
					<V ID="Fin"><VV>uffzogen,</VV></V>
				</praed></lb>
		</line>
		<line n="8">
			<lb n="4,5,3300">
