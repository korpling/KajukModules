	<pb n="38">
		<line n="1">
			<lb>
				<VOR>Im Jahr 1640 umb das Neue Jahr da</VOR>
				<praed>
					<V ID="Fin"><VV>kamen</VV></V>
				</praed> in das darmstädische Land <subj>eine Armate schwedisch
			Volk.</subj></lb><newline></newline>
		</line>
		<line n="2">
			<lb n="3000,2000">
				<VOR>Die</VOR>
				<praed>
					<V ID="Fin"><HV>hatte</HV></V>
				</praed>
				<praed>
					<V ID="PII"><VV>gefuhret</VV></V>
				</praed>
				<subj>ein Hertzog von Weinmar.</subj></lb>
		</line>
		<line n="3">
			<lb n="1,3000,2000">
				<subj type="E" dir="V" change="indef-def,MF-VF">der Hertzog von Weinmar</subj>
				<praed>
					<V ID="Fin"><KV>War</KV></V>
				</praed><newline></newline>
				<J IR="adv">
					<AP>aber</AP>
				</J>
				<praed>
					<V ID="PII"><VV>gestorben</VV></V>
				</praed></lb>
			<lb n="1">
				<J IR="kons">
					<KON>und</KON>
				</J>
				<praed>
					<V ID="Fin"><VV>führet</VV></V>
				</praed> sie <subj>ein ander Oberster aus Frankreich.</subj></lb>
			<lb>
				<subj>Die</subj>
				<praed>
					<V ID="Fin"><VV>lagen</VV></V>
				</praed><newline></newline> in dem Land biß umb Pilipi Jacobi.</lb>
		</line>
		<line n="4">
			<lb>
				<subj>Sie</subj>
				<praed>
					<V ID="Fin"><KV>waren</KV></V>
				</praed>
				<praed>
					<ADJGr>24 Regement star[k] zu Roß und zu Fuß;</ADJGr>
				</praed></lb><newline></newline>
		</line>
		<line n="5">
			<lb>
				<praed real="mikro">
					<V ID="Fin"><VV>nahmen</VV></V>
				</praed> alles Vieh,</lb>
			<lb n="2,3001,3341">
				<SUB IR="zero">was</SUB>
